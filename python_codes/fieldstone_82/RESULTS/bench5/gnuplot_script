plot[][]\
'solve_nq2.ascii' u 6:3 w lp t 'nq=2^2',\
'solve_nq3.ascii' u 6:3 w lp t 'nq=3^2',\
'solve_nq4.ascii' u 6:3 w lp t 'nq=4^2',\
60 lw .5 lt -1 dt 2 t '1 min',\
600 lw .5 lt -1 dt 4 t '10 min'

set output 'build.pdf'
