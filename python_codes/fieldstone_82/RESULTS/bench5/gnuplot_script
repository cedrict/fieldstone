'vels_nq3.ascii' u 7:4 w lp lt 2 dt 3 t 'max(v), nq=2^3',\
'vels_nq4.ascii' u 7:3 w lp lt 3 dt 2 t 'min(v), nq=2^3',\
'vels_nq4.ascii' u 7:4 w lp lt 3 dt 3 t 'max(v), nq=2^3',\

