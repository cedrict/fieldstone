'build_nq2.ascii' u 7:4 w lp t 'nq=2^2',\
'build_nq3.ascii' u 7:4 w lp t 'nq=3^2',\
'build_nq4.ascii' u 7:4 w lp t 'nq=4^2',\
60 lw .5 lt -1 dt 2 t '1 min'

