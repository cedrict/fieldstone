#!/bin/bash

rm errors_*.ascii
   
for CFL in 2 1 0.75 0.5 0.25 .125 0.0625 0.03125 ; do

    for method in RK1 RK2 RK3 RK4 RK4_38 RKF ODE87 ; do

      echo method= $method CFL= $CFL

      python3 stone.py 256 $CFL  $method 0 > opla

      grep err opla >> errors_${method}.ascii

   done

done



