#
set term pdf enhanced font 'Times,12pt' ps .4
set xlabel '{/Symbol q}'
set grid
set key outside
set log y

cm=0.01
year=365.25*3600*24

ymin=1e-12
ymax=1e-3

#################################################################

set ylabel 'velocity magnitude (cm/year)'

