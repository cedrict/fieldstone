#
set term pdf enhanced font 'Times,12pt' linewidth .1
set xlabel '{/Symbol q}'
set grid
set key outside
set log y

cm=0.01
year=365.25*3600*24

ymin=1e-7
ymax=0.6


set title 'nelr=32, mapping=Q_4, nqperdim=4'
set output 'vel_64_xi.pdf'
plot[0:pi][ymin:ymax]\
'vel_R2.ascii_64_xi5_nq4_m4' u 1:($2/cm*year) w l lt 1 t 'xi=5',\
'vel_R2.ascii_64_xi6_nq4_m4' u 1:($2/cm*year) w l lt 2 t 'xi=6',\
'vel_R2.ascii_64_xi7_nq4_m4' u 1:($2/cm*year) w l lt 3 t 'xi=7',\
