#
set term pdf enhanced font 'Times,12pt' linewidth .1
set xlabel '{/Symbol q}'
set grid
set key outside
set log y

cm=0.01
year=365.25*3600*24

ymin=1e-6
ymax=1e1

#################################################################

set ylabel 'velocity magnitude (cm/year)'

set title 'nelr=8, mapping=Q_2'
set output 'vel_16_m2.pdf'
plot[0:pi][ymin:ymax]\
'vel_R2.ascii_16_xi6_nq3_m2' u 1:($2/cm*year) w l lt 1 t 'nq=3',\
'vel_R2.ascii_16_xi6_nq4_m2' u 1:($2/cm*year) w l lt 2 t 'nq=4',\
'vel_R2.ascii_16_xi6_nq5_m2' u 1:($2/cm*year) w l lt 3 t 'nq=5',\

set title 'nelr=8, mapping=Q_3'
set output 'vel_16_m3.pdf'
