'vel_R2.ascii_16_xi6_nq3_m3' u 1:($2/cm*year) w l lt 1 t 'nq=3',\
'vel_R2.ascii_16_xi6_nq4_m3' u 1:($2/cm*year) w l lt 2 t 'nq=4',\
'vel_R2.ascii_16_xi6_nq5_m3' u 1:($2/cm*year) w l lt 3 t 'nq=5',\

