#
set term pdf enhanced font 'Times,12pt' linewidth .1
set xlabel '{/Symbol q}'
set grid
set key outside

cm=1
year=1

xmin=0
xmax=pi
ymin=-25
ymax=300

#################################################################

set ylabel 'Dynamic topography (m)'

set title 'nelr=8, mapping=Q_2'
set output 'd_t_16_m2.pdf'
plot[xmin:xmax][ymin:ymax]\
'd_t_R2.ascii_16_xi6_nq3_m2' u 1:($2/cm*year) w l lt 1 t 'nq=3',\
'd_t_R2.ascii_16_xi6_nq4_m2' u 1:($2/cm*year) w l lt 2 t 'nq=4',\
'd_t_R2.ascii_16_xi6_nq5_m2' u 1:($2/cm*year) w l lt 3 t 'nq=5',\

