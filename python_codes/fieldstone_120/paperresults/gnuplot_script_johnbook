#################################################################
set log x
set log y
set format y "10^{%L}"
set title "unstructured mesh"
set xlabel 'h'

set ylabel '||u-u_h||_{L_2}'
set output 'johnbook_unstructured_errorsV.pdf'
plot[hmin:hmax][]\
'johnbook/unstructured/errors_P1+_P1'  u 16:6 w lp ps .5 lt 3 t 'P_1^+xP_1',\
'johnbook/unstructured/errors_P2_P1'   u 16:6 w lp ps .5 lt 4 t 'P_2xP_1',\
'johnbook/unstructured/errors_P2_P0'   u 16:6 w lp ps .5 lt 5 t 'P_2xP_0',\
'johnbook/unstructured/errors_P2+_P-1' u 16:6 w lp ps .5 lt 6 t 'P_2^+xP_{-1}',\
x**2*10 lt -1 dt 3 t 'h^2',\
x**3*5  lt -1 dt 2 t 'h^3'

set ylabel '||p-p_h||_{L_2}'
set output 'johnbook_unstructured_errorsP.pdf'
plot[hmin:hmax][]\
'johnbook/unstructured/errors_P1+_P1'  u 16:9 w lp ps .5 lt 3 t 'P_1^+xP_1',\
'johnbook/unstructured/errors_P2_P1'   u 16:9 w lp ps .5 lt 4 t 'P_2xP_1',\
'johnbook/unstructured/errors_P2_P0'   u 16:9 w lp ps .5 lt 5 t 'P_2xP_0',\
'johnbook/unstructured/errors_P2+_P-1' u 16:9 w lp ps .5 lt 6 t 'P_2^+xP_{-1}',\
x**1*2    lt -1 dt 5 t 'h^1',\
x**1.5*65 lt -1 dt 4 t 'h^{1.5}',\
x**2*2    lt -1 dt 3 t 'h^2'

