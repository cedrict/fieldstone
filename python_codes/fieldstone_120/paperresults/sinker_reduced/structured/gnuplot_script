#
set term pdf enhanced font "Times,17pt" ps .3
set grid
set log x
set key outside
set xlabel '{/Symbol h}^*'
#set ylabel 'v_y/{/Symbol d}{/Symbol r}'
#set ylabel '{/Symbol n}'
set ylabel 'v^*'
set format x "10^{%L}"

##################################################################
set title '16x16'
set output 'sinker_reduced_vel_16.pdf'
plot[1e-4:1e4][-0.01:0]\
'sinker_vel_Q2_Q1_16'   u 6:($5/$7) w lp t 'Q_2xQ_1',\
'sinker_vel_Q2_Pm1_16'  u 6:($5/$7) w lp t 'Q_2xP_{-1}',\
'sinker_vel_P1+_P1_16'  u 6:($5/$7) w lp t 'P_1^+xP_1',\
'sinker_vel_P2_P1_16'   u 6:($5/$7) w lp t 'P_2xP_1',\
'sinker_vel_P2_P0_16'   u 6:($5/$7) w lp t 'P_2xP_0',\
'sinker_vel_P2+_P-1_16' u 6:($5/$7) w lp t 'P_2^+xP_{-1}',\
'my_point_values_8.dat' u ($2/1e21):($8/($3-3200)*12) w l lt -1 dt 2 t 'ASPECT'

set ylabel 'p/{/Symbol d}{/Symbol r}/g/L_b'
set output 'sinker_reduced_press_16.pdf'
plot[1e-4:1e4][-0.09:-0.06]\
'sinker_press_Q2_Q1_16'   u 6:($5/$7/0.25) w lp t 'Q_2xQ_1',\
'sinker_press_Q2_Pm1_16'  u 6:($5/$7/0.25) w lp t 'Q_2xP_{-1}',\
'sinker_press_P1+_P1_16'  u 6:($5/$7/0.25) w lp t 'P_1^+xP_1',\
'sinker_press_P2_P1_16'   u 6:($5/$7/0.25) w lp t 'P_2xP_1',\
'sinker_press_P2_P0_16'   u 6:($5/$7/0.25) w lp t 'P_2xP_0',\
'sinker_press_P2+_P-1_16' u 6:($5/$7/0.25) w lp t 'P_2^+xP_{-1}',\
#'my_point_values_8.dat' u ($2/1e21):($9/$3/10/128e3) w lp ps .5 lt -1 t 'ASPECT, 256^2',\


