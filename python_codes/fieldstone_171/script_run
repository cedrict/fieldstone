#! /bin/bash


for m in 'alpha' 'beta' 'gamma' 'delta' 'epsilon' 'zeta' 'eta' 'theta' 'iota' 'kappa' 'lambda' 'mu' 'nu' 'xi' 'pi' 'rho' 'sigma'; do 
    echo $m
    python3 stone.py  $m 257 > opla_$m
done

