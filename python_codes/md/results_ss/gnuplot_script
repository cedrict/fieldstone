# gnuplot script
set term pdf enhanced font "Times,12pt"
set grid
set key outside

set xlabel '# iter'

###############################################################################

set ylabel 'Nu'
set output 'Nu_Ra1e4.pdf'
set title 'Ra=10^4'
plot[][]\
'ss_Nu_vrms_10000_16.ascii' u 1:2 w l t '16x16',\
'ss_Nu_vrms_10000_32.ascii' u 1:2 w l t '32x32',\
'ss_Nu_vrms_10000_48.ascii' u 1:2 w l t '48x48',\
'ss_Nu_vrms_10000_64.ascii' u 1:2 w l t '64x64',\
'ss_Nu_vrms_10000_80.ascii' u 1:2 w l t '80x80',\
'ss_Nu_vrms_10000_96.ascii' u 1:2 w l t '96x96',\
4.884409 lt -1 dt 2

set ylabel 'v_{rms}'
set output 'vrms_Ra1e4.pdf'
set title 'Ra=10^4'
plot[][]\
'ss_Nu_vrms_10000_16.ascii' u 1:3 w l t '16x16',\
'ss_Nu_vrms_10000_32.ascii' u 1:3 w l t '32x32',\
'ss_Nu_vrms_10000_48.ascii' u 1:3 w l t '48x48',\
'ss_Nu_vrms_10000_64.ascii' u 1:3 w l t '64x64',\
'ss_Nu_vrms_10000_80.ascii' u 1:2 w l t '80x80',\
'ss_Nu_vrms_10000_96.ascii' u 1:2 w l t '96x96',\
42.864947 lt -1 dt 2

