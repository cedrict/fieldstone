#! /bin/bash

rm timings_solve_V4
rm timings_build_V4
rm timings_errors_V4
rm timings_normalise_V4
rm errors_V4

for n in {8..48} ; do
    for s in {1..2} ; do 
        echo nelx=nely= $n
        python3 stone_V4.py  $n > opla
        grep solve opla >> timings_solve_V4
        grep FE opla >> timings_build_V4
        grep errors opla >> timings_errors_V4
        grep normalise opla >> timings_normalise_V4
        grep errv opla >> errors_V4
    done
done

for n in 56 64 72 80 96 112 128 ; do
    for s in {1..3} ; do 
        echo nelx=nely= $n
        python3 stone_V4.py  $n > opla
        grep solve opla >> timings_solve_V4
        grep FE opla >> timings_build_V4
        grep errors opla >> timings_errors_V4
        grep normalise opla >> timings_normalise_V4
        grep errv opla >> errors_V4
    done
done




